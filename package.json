{
  "name": "cryptypassta",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "engines": {
    "node": ">=10.19.0"
  },
  "scripts": {
    "start": "concurrently \"npm run-script start:blockchain\" \"npm run-script start:main-app\"",
    "start:blockchain": "concurrently \"npm run-script build:contract\" \"npm run-script migrate\"",
    "build:contract": "ganache-cli -p 9418 -m 'pulp rack antenna patient machine arrest endless ten unusual spice load enlist'",
    "migrate": "cd ./src/blockchain && truffle migrate",
    "test": "cd ./src/blockchain && truffle test",
    "install": "concurrently \"cd src/backend && npm install\" \"cd src/frontend/my-app && npm install\"",
    "start:main-app": "concurrently \"cd src/backend && npm run start-dev\" \"cd src/frontEnd/my-app && npm start\"",
    "install:backend": "cd src/backend && npm install",
    "install:frontend": "cd src/frontend/my-app && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianleck/cryptyPassta.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ianleck/cryptyPassta/issues"
  },
  "homepage": "https://github.com/ianleck/cryptyPassta#readme",
  "dependencies": {
    "@openzeppelin/contracts": "^2.5.0",
    "ganache-cli": "^6.9.1",
    "truffle": "^5.1.18",
    "truffle-assertions": "^0.9.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.2"
  }
}
