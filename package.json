  "dependencies": {
    "@openzeppelin/contracts": "^2.5.0",
    "ganache-cli": "^6.9.1",
    "npm-run-all": "^4.1.5",
    "truffle": "^5.1.20",
    "truffle-assertions": "^0.9.2"
  }
}
